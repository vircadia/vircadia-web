        "unique-names-generator": "^4.7.1",
        "vue": "^3.5.3",
        "vue-router": "^4.4.3",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@jitsi/vue-sdk": "^1.0.4",
        "@quasar/app-vite": "^2.4.0",
        "@types/dompurify": "^3.0.2",
        "@types/node": "^12.20.21",
