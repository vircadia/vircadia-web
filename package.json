        "@vircadia/web-sdk": "^2024.1.0",
        "@vueuse/core": "^11.0.3",
        "ammojs-typed": "^1.0.6",
        "console-log-colors": "^0.5.0",
        "dompurify": "^3.0.3",
        "dotenv": "^17.2.1",
        "eight-colors": "^1.3.0",
        "mobx": "^6.10.0",
        "pinia": "^2.2.2",
        "quasar": "^2.18.5",
